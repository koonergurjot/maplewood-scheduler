{
  "name": "maplewood-scheduler",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:ci": "tsc -p tsconfig.ci.json && vite build",
    "preview": "vite preview --strictPort",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prebuild": "npm run typecheck",
    "clean:fs:dry": "tsx scripts/cleanupFs.ts",
    "clean:fs:apply": "tsx scripts/cleanupFs.ts --apply",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier -w .",
    "test": "vitest run",
    "test:watch": "vitest",
    "depcheck": "depcheck --skip-missing true"
  },
  "dependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "chart.js": "^4.4.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "papaparse": "^5.4.1",
    "pdfkit": "^0.17.2",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.7.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "jsdom": "^24.0.0",
    "prettier": "^3.3.3",
    "tsx": "^4.19.0",
    "typescript": "^5.4.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "globals": "^15.6.0",
    "vite": "^5.2.0",
    "vitest": "^1.6.0",
    "depcheck": "^1.4.7"
  },
  "engines": {
    "node": "18.20.8"
  }
}
